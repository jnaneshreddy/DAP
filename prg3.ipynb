    "    _x = 5\n",
    "class B(A):\n",
    "    def __init__(self,y):\n",
    "        self._y = y\n",
    "    def __add__(self,other):\n",
    "        return self._y + other._x\n",
    "    def display(self):\n",
    "        print(f\"x={self._x} y={self._y}\")\n",
    "obj1=B(5)\n",
    "obj2=B(6)\n",
    "\n",
    "obj1.display()\n",
    "obj2.display()\n",
    "\n",
    "print(obj1+obj2)"
